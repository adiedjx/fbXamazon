    <exception-type>java.lang.Throwable</exception-type>
    <location>/my/error.jsp</location>
  </error-page>
  <servlet>
    <servlet-name>address</servlet-name>
    <jsp-file>/my/address.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>address</servlet-name>
    <url-pattern>/my/address</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>admin</servlet-name>
    <jsp-file>/my/admin.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>admin</servlet-name>
    <url-pattern>/my/admin</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>cart</servlet-name>
    <jsp-file>/my/cart.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>cart</servlet-name>
    <url-pattern>/my/cart</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>editprofile</servlet-name>
    <jsp-file>/my/editprofile.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>editprofile</servlet-name>
    <url-pattern>/my/editprofile</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>inputproduct</servlet-name>
    <jsp-file>/my/inputproduct.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>inputproduct</servlet-name>
    <url-pattern>/my/inputproduct</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <jsp-file>/my/login.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/my/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>product</servlet-name>
    <jsp-file>/my/product.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>product</servlet-name>
    <url-pattern>/my/product</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>signup</servlet-name>
    <jsp-file>/my/signup.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>signup</servlet-name>
    <url-pattern>/my/signup</url-pattern>
  </servlet-mapping>
  <servlet>
