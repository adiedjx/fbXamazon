    <servlet-name>newsfeed</servlet-name>
    <url-pattern>/my/newsfeed</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>shop</servlet-name>
    <jsp-file>/my/shop.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>shop</servlet-name>
    <url-pattern>/my/shop</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>time</servlet-name>
    <jsp-file>/my/timeline.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>time</servlet-name>
    <url-pattern>/my/timeline</url-pattern>
  </servlet-mapping>
</web-app>